= bootstrap_form_for [:lti, @tool, @launch] do |f|
  - if @launch.errors.any?
    #error_explanation
      %h2= "#{pluralize(@launch.errors.count, "error")} prohibited this tool_launch from being saved:"
      %ul
        - @launch.errors.full_messages.each do |message|
          %li= message

  = f.text_field :jwt
  = f.text_area :decoded_jwt
  = f.submit 'Save'